{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "importOrder": [
    "^(?!\\.|/|src).*",
    "^(?!.*\\/(types|types\\.)|.*\\.(css|scss)$|.*\\.(svg|png|webp|jpg|gif|jpeg)$).*",
    "\\/types$",
    "\\.(css|scss)$",
    "\\.(svg|png|webp|jpg|gif|jpeg)$"
  ],
  "importOrderSeparation": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ]
}
